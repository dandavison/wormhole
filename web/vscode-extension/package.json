{
  "name": "wormhole",
  "displayName": "Wormhole",
  "description": "Wormhole message channel for editor operations",
  "publisher": "dandavison",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Wormhole",
      "properties": {
        "wormhole.port": {
          "type": "number",
          "default": 7117,
          "description": "Wormhole server port"
        }
      }
    }
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "vscode:prepublish": "npm run compile"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.71.0",
    "typescript": "^4.9.5"
  }
}
